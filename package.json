{
  "name": "flag-api",
  "version": "1.0.0",
  "description": "Lightswitch API",
  "main": "index.js",
  "scripts": {
    "deploy:remote": "gcloud run deploy --image gcr.io/pristine-flames-265923/flag-api --platform managed",
    "build:remote": "gcloud builds submit --tag gcr.io/pristine-flames-265923/flag-api",
    "start": "node dist",
    "build": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "lint": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet",
    "lint:fix": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest"
  },
  "dependencies": {
    "basic-auth": "^2.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.6.6",
    "express-pino-logger": "^4.0.0",
    "express-session": "^1.17.0",
    "knex": "^0.20.8",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "pino": "^5.16.0",
    "sequelize": "^5.21.3",
    "uuid-apikey": "^1.4.6",
    "uuidv4": "^6.0.2"
  },
  "devDependencies": {
    "@types/basic-auth": "^1.1.2",
    "@types/bluebird": "^3.5.29",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-pino-logger": "^4.0.2",
    "@types/jest": "^24.9.1",
    "@types/pino": "^5.8.13",
    "@types/uuid-apikey": "^1.4.0",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "pino-pretty": "^3.5.0",
    "prettier": "^1.19.1",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^25.0.0",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.4"
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
}
